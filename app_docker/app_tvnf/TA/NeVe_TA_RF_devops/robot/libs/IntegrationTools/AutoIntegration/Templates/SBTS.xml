<init-configuration>

    <!-- SBTS NE version:SBTS16.2-->
    <NEType adaptation="com.nokia.sbts" version="SBTS16.2"
            class="SBTS"> <!-- change the version to the actual used version -->
        <NEInstance agentAddress="****">                     <!-- change the agentAddress **** to the actual used IP -->

            <MR distName="MRC-****/MR-****">             <!-- change the MR to the actual used MR -->
                <MO distName="PLMN-****/SBTS-****">  <!-- change the distName **** to the actual SBTS FQDN -->
                    <version>SBTS16.2</version>
                    <!-- change the version to the actual used version -->
                </MO>
            </MR>

            <!-- SBTS MO part -->
            <MO distName="****/NE3SWS-1">              <!-- change the distName **** to the actual SBTS FQDN -->
                <hostName>****</hostName>
                <!-- change the hostName **** to the actual used IP or Domain name-->
                <baseServiceName>NE3S/1.0</baseServiceName>
                <ne3sBasicOperationsService>NE3SBasicOperationsService</ne3sBasicOperationsService>
                <ne3sBulkOperationsService>NE3SBulkOperationsService</ne3sBulkOperationsService>
                <ne3sOperationService>NE3SOperationService</ne3sOperationService>
                <ne3sRegistrationService>NE3SRegistrationService</ne3sRegistrationService>
                <ne3sSessionService>NE3SSessionService</ne3sSessionService>
                <httpPort>****</httpPort>
                <!-- change the httpPort **** to the actual http port, default is 8080 -->
                <httpsPort>****</httpsPort>
                <!-- change the httpsPort **** to the actual https port, default is 8443 -->
                <ipVersion>****</ipVersion>
                <!-- change the ipVersion **** to the actual ip version. supported (0=IPv4(default), 1=IPv6  -->
                <securityMode>****</securityMode>
                <!-- change the securityMode **** to the actual securityMode. TLS (Transport Layer Security) supported (0=no, 1=yes (default)) -->
            </MO>

        </NEInstance>
    </NEType>
</init-configuration>